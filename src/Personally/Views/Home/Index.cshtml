@model IEnumerable<Personally.Models.Restaurant>

<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>Angela's favorite Restaurant</title>
</head>
<body>
    <h1>Welcome!</h1>
    <table>
        @foreach (var restaurant in Model)
        {
        <tr>
            <td>@restaurant.Id</td>
            <td>@restaurant.Name</td>
        </tr>
        }
    </table>
</body>
</html>